{
  "name": "souljs",
  "version": "2.6.0",
  "description": "A nodejs framework for building concise and decorative applications written by typescript",
  "main": "dist/index.js",
  "scripts": {
    "format": "prettier *.ts **/*.ts **/**/*.ts **/**/**/*.ts  --ignore-path ./.prettierignore --write && git status",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "publish": "npm run build && npm publish",
    "build": "tsc -p tsconfig.build.json",
    "sample": "ts-node sample/src/main.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/my-soul/souljs.git"
  },
  "keywords": [
    "souljs"
  ],
  "author": "renkun",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/my-soul/souljs/issues"
  },
  "homepage": "https://github.com/my-soul/souljs#readme",
  "devDependencies": {
    "@types/debug": "0.0.31",
    "@types/glob": "^7.1.1",
    "@types/joi": "^14.3.0",
    "@types/koa": "^2.0.48",
    "@types/koa-bodyparser": "^4.2.1",
    "@types/koa-conditional-get": "^2.0.0",
    "@types/koa-etag": "^3.0.0",
    "@types/koa-hbs": "^1.0.4",
    "@types/koa-helmet": "^3.1.2",
    "@types/koa-mount": "^3.0.1",
    "@types/koa-router": "^7.0.37",
    "@types/koa-session": "^5.7.4",
    "@types/koa-static": "^4.0.0",
    "@types/node": "^10.12.19",
    "@types/swagger-ui-dist": "^3.0.0",
    "prettier": "1.15.3",
    "ts-node": "^7.0.1",
    "tslint": "5.12.1",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "@koa/cors": "^2.2.3",
    "debug": "^4.1.1",
    "glob": "^7.1.3",
    "joi": "^14.3.1",
    "joi-to-json-schema": "^3.5.0",
    "koa": "^2.6.2",
    "koa-bodyparser": "^4.2.1",
    "koa-conditional-get": "^2.0.0",
    "koa-etag": "^3.0.0",
    "koa-hbs": "^1.0.0",
    "koa-helmet": "^4.0.0",
